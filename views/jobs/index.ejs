<%- include('../partials/header') %>
    <table id="list">
        <thead>
            <tr>
                <th>Company &nbsp;<br></th> <br>

                <th>Job Title</th>
                <th>Detail</th>
                <th>Delete</th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            <% jobs.forEach(function(j) { %>
                <tr>
                    <td>
                        <%= j.company %>
                    </td>
                    <td>
                        <%= j.title %>
                    </td>
                    <td><a href="/jobs/<%= j._id %>">DETAILS</a></td>
                    <form action="/jobs/<%= j.id %>?_method=DELETE" method="POST" class="delete">
                        <button type="submit">X</button>
                </tr>
                <% }); %>
        </tbody>
    </table>

    <%- include('../partials/footer') %>

